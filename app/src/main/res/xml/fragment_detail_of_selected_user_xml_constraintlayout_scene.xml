<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000"
        motion:staggered=".55"
        motion:autoTransition="animateToStart">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/profile_picture_detail_of_user"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="140dp"
            android:layout_height="140dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="60dp" />
        <Constraint
            android:id="@+id/username_detail_of_user"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/profile_picture_detail_of_user"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="8dp" />
        <Constraint
            android:id="@+id/fullname_detail_of_user"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/username_detail_of_user"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/company_detail_of_user"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/fullname_detail_of_user"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="8dp" />
        <Constraint
            android:id="@+id/location_detail_of_user"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/company_detail_of_user"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/repository_detail_of_user"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/location_detail_of_user"
            motion:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/profile_picture_detail_of_user"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="140dp"
            android:layout_height="140dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="60dp" />
        <Constraint
            android:id="@+id/username_detail_of_user"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/profile_picture_detail_of_user"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="8dp"
            android:alpha="5" />
        <Constraint
            android:id="@+id/fullname_detail_of_user"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/username_detail_of_user"
            motion:layout_constraintStart_toStartOf="parent"
            android:alpha="5" />
        <Constraint
            android:id="@+id/company_detail_of_user"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/fullname_detail_of_user"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="8dp"
            android:alpha="5" />
        <Constraint
            android:id="@+id/company_detail_of_user"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/fullname_detail_of_user"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="8dp"
            android:alpha="1" />
        <Constraint
            android:id="@+id/location_detail_of_user"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/company_detail_of_user"
            motion:layout_constraintStart_toStartOf="parent"
            android:alpha="5" />
        <Constraint
            android:id="@+id/repository_detail_of_user"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/location_detail_of_user"
            motion:layout_constraintStart_toStartOf="parent"
            android:alpha="5" />
    </ConstraintSet>
</MotionScene>